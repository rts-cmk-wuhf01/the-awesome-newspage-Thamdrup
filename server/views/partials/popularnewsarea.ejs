<div class="section-heading">
	<h6>Popular News</h6>
</div>
<div class="row">
<!-- Single Post -->
<% popularStories.forEach(popularStory=>{%>
	<div class="col-12 col-md-6">
		<div class="single-blog-post style-3">
			<div class="post-thumb">
				<a href="#"><img src="<%= popularStory.img%>" alt=""></a>
			</div>
			<div class="post-data">
				<a href="#" class="post-catagory"><%= popularStory.category %></a>
				<a href="#" class="post-title">
					<h6><%= popularStory.title %></h6>
				</a>
				<div class="post-meta d-flex align-items-center">
					<a href="#" class="post-like"><img src="<%= popularStory.imgLike%>" alt="">
						<span><%= popularStory.likes %></span></a>
					<a href="#" class="post-comment"><img src="<%= popularStory.imgComments%>" alt="">
						<span><%= popularStory.comments %></span></a>
				</div>
			</div>
		</div>
	</div>
<% }) %> 
</div>