<div class="container">
	<div class="row">


	<!-- Single Team Member -->
		<% currentEmployees.forEach(employee =>{%>
			<div class="col-12 col-sm-6 col-lg-3">
				<div class="single-team-member">
					<img src="<%= employee.imgSrc %>" alt="">
					<div class="team-info">
						<h2><%= employee.name %></h2>
						<p><%= employee.position %></p>
					</div>
				</div>
			</div>
		<% }) %>


	</div>
</div>